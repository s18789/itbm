@{
    var patients = (IEnumerable<Patient>)ViewData["Patients"];
}

<table>
    <tr>
        <th>
            Personal Number:
        </th>
        <th>
            FirstName:
        </th>
        <th>
            LastName:
        </th>
        <th>
            Action
        </th>
    </tr>
    @foreach (Patient patient in patients)
    {
        <tr>
            <td>
                @patient.PersonalNumber
            </td>
            <td>
                @patient.FirstName
            </td>
            <td>
                @patient.LastName
            </td>
            <td>

            </td>
            <td>
                 | 
            </td>
            <td>

            </td>
        </tr>
    }
</table>
